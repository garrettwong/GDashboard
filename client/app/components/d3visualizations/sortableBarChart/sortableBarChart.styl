.axis text {
  font: 10px sans-serif;
}

.axis path,
.axis line {
  fill: none;
  stroke: #000;
  shape-rendering: crispEdges;
}

.bar {
  fill: mediumslateblue;
  fill-opacity: .9;
}


x.axis path {
  
}

.line {
  fill: none;
  stroke: steelblue;
  stroke-width: 1.5px;
}

.line2 {
  fill: none;
  stroke: mediumspringgreen;
  stroke-width: 6.5px;
}

  
.legend rect {
  fill: white; 
  stroke: black;
  opacity: 0.8;
}